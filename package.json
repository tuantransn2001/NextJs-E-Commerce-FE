    "framer-motion": "10.12.16",
    "joi": "17.9.2",
    "moment": "2.29.4",
    "next": "13.4.6",
    "next-redux-wrapper": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.0",
    "react-paginate": "8.2.0",
    "react-redux": "8.1.1",
    "sass": "1.63.5",
    "socket.io-client": "4.7.2",
    "styled-components": "5.3.10",
    "styled-reset": "4.4.7",
